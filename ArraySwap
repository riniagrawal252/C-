    }
    cout<<"\n Number of Elements "<<ar.size()<<endl;
    cout<<"\n Total size in Bytes "<<sizeof(ar)<<endl;
    cout<<"Starting Element: "<<ar.front()<<endl;
    cout<<"Last Element: "<<ar.back()<<endl;
}
